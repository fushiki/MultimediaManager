<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:vm="clr-namespace:MultimediaManager.ViewModels"
                    xmlns:vw="clr-namespace:MultimediaManager.Views"
                    >
    <DataTemplate DataType="{x:Type vm:TagsViewModel}">
        <vw:TagsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DuplicateViewModel}">
        <vw:DuplicateView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TestDragAndDropViewModel}">
        <vw:TestDragAndDropView/>
    </DataTemplate>
</ResourceDictionary>